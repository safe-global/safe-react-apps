{
  "name": "safe-apps",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start-compound": "dotenv -e .env -- yarn workspace compound start",
    "start-tx-builder": "dotenv -e .env -- yarn workspace tx-builder start",
    "start-wallet-connect": "dotenv -e .env -- yarn workspace wallet-connect start",
    "build-compound": " dotenv -e .env -- yarn workspace compound build",
    "build-tx-builder": "dotenv -e .env -- yarn workspace tx-builder build",
    "build-wallet-connect": "dotenv -e .env -- yarn workspace wallet-connect build"
  },
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "dotenv-cli": "^4.0.0",
    "typescript": "^4.1.3"
  }
}
