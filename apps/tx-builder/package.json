{
  "name": "tx-builder",
  "version": "1.8.0",
  "private": true,
  "homepage": "/tx-builder",
  "dependencies": {
    "@gnosis.pm/safe-deployments": "^1.11.0",
    "@gnosis.pm/safe-react-components": "^1.1.4",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.26.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "react-beautiful-dnd": "^13.1.0",
    "react-hook-form": "^7.28.0",
    "react-router-dom": "^6.2.1",
    "react-virtuoso": "^2.8.2",
    "web3": "~1.7.0"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@types/react-beautiful-dnd": "^13.1.2",
    "ethers": "^5.5.2",
    "hardhat": "^2.7.0",
    "hardhat-deploy": "^0.11.10"
  },
  "scripts": {
    "start": "dotenv -e .env -- react-app-rewired start",
    "build": "dotenv -e .env -- react-app-rewired build",
    "test": "react-app-rewired test --passWithNoTests",
    "contract:compile": "npx hardhat compile",
    "contract:verify": "npx hardhat verify",
    "contract:verify:rinkeby": "npx hardhat --network rinkeby verify",
    "basic:contract:deploy": "npx hardhat run src/scripts/deploy-test-basic-contract.js",
    "basic:contract:deploy:rinkeby": "npx hardhat run --network rinkeby src/scripts/deploy-test-basic-contract.js",
    "matrix:contract:deploy": "npx hardhat run src/scripts/deploy-test-matrix-contract.js",
    "matrix:contract:deploy:rinkeby": "npx hardhat run --network rinkeby src/scripts/deploy-test-matrix-contract.js",
    "contract:deploy-all": "hardhat deploy-test-contracts --network",
    "deploy:s3": "bash ../../scripts/deploy_to_s3_bucket.sh",
    "deploy:pr": "bash ../../scripts/deploy_pr.sh",
    "deploy:prod-hook": "bash ../../scripts/prepare_production_deployment.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
