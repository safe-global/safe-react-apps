{
  // file versioning for future migrations
  "version": "1.0",
  // tx builder version for debug purposes
  "txBuilderVersion": "1.0.0", 
  // chain Id to validate the chain where the batch was created
  "chainId": "1",
  // checksum including information like `chainId`, `transactions`.... to warn the user when the file was manipulated
  "checksum": "0xa1b0c2", 
  // UNIX timestamp when the file was exported
  "createdAt": 0,
  // Conected Safe at the time the file was exported. We would be able to show warnings if using a file from another person
  "createdFromSafeAddress": "0x0000",
  // owner that was conected when file was exported. This would be OPTIONAL as the owner could be disconnected
  "createdFromOwnerAddress": "0x0000", // OPTIONAL
  // A name to show when importing the batch, could be use to name when saving to the local storage library later
  "name": "Your batch",
  // A description to add more information. This would be OPTIONAL
  "description": "",// OPTIONAL
  // The transaction batch will start here
  "transactions": [
    // Transaction example for 1 ETH transfer
    {
      "to": "0xd9Db270c1B5E3Bd161E8c8503c55cEABeE709552",
      "value": "1000000000000000000",
      "data": "0x"
    },
    // Transaction example for a method call without inputs. Decoding for TX Builder would be possible
    {
      "to": "0xd9Db270c1B5E3Bd161E8c8503c55cEABeE709552",
      "value": "0",
      "contractMethod": {
          "inputs": [],
          "name": "fallback",
          "payable": false
      }
    },
    // Transaction example for contract method with inputs. Decoding for TX Builder would be possible
    {
      "to": "0xd9Db270c1B5E3Bd161E8c8503c55cEABeE709552",
      "value": "0",
      "contractMethod": {
          "inputs": [
              {
                  "internalType": "address",
                  "name": "prevOwner",
                  "type": "address"
              },
              {
                  "internalType": "address",
                  "name": "owner",
                  "type": "address"
              },
              {
                  "internalType": "uint256",
                  "name": "_threshold",
                  "type": "uint256"
              }
          ],
          "name": "removeOwner",
          "payable": false
      },
      // Values for the inputs on the method above
      "contractInputsValues": {
          "prevOwner": "0xd9Db270c1B5E3Bd161E8c8503c55cEABeE709552",
          "owner": "0xd9Db270c1B5E3Bd161E8c8503c55cEABeE709552",
          "_threshold": "1"
      }   
    },
    // Transaction example with RAW data. Decoding for TX Builder would not be possible, just show the data
    {
      "to": "0xd9Db270c1B5E3Bd161E8c8503c55cEABeE709552",
      "value": "0",
      "data": "0xe19a9dd9000000000000000000000000d9db270c1b5e3bd161e8c8503c55ceabee709552"
